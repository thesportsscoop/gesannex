{
  "name": "ges-annex-website",
  "version": "1.0.0",
  "private": true,
  "description": "GES Annex - Educational Portal with Eleventy and React",
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "keywords": [],
  "scripts": {
    "start": "npm run build:react && eleventy --serve",
    "build": "npm run build:react && eleventy && npm run copy-react",
    "build:react": "react-scripts build",
    "copy-react": "shx cp -r build/* _site/",
    "test": "react-scripts test",
    "test:react": "react-scripts test",
    "eject:react": "react-scripts eject"
  },
  "dependencies": {
    "firebase": "^11.9.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.1",
    "shx": "^0.3.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
